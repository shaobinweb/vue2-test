.demand{
  width:100%;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  background:rgb(158, 153, 153);
  align-items: center;
  margin-bottom: 30rpx;
}
.demand-left{
  display: flex;
  align-items: center;
  /* padding: 0 15rpx; */
}
.demand-left image{
  margin:0 20rpx 0 18rpx;
}
.demand-right{
  padding: 6rpx 10rpx;
  border-radius: 5rpx;
  margin-left: 30rpx;
  background:rgb(121, 58, 7);
  color: #fff;
  font-size: 24rpx;
}
.times{
  width:100%;
  height: 100rpx;
  background: #eee;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx;
}
.time-content{
  display: flex;
  background:#eee;
}
.section__title{
  display: flex;
  align-items: center;
  align-content: center;
}
.section__title .red{
  color:red;
  padding: 0 10rpx;
}
.section__title .font{
  font-size: 26rpx;
  height: 53rpx;
  width:184rpx;
  line-height: 53rpx;
  /* padding-right: 20rpx; */
}
input{
  width:460rpx;
}
.section{
  display: flex;
}
.qued{
  width: 80%;
  margin: 50rpx auto;
}